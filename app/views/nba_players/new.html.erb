<%= form_with url: "/nbateams/#{@nbateam.id}/players", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %> 

  <%= form.label :position %>
  <%= form.text_field :position %>

  <%= form.label :age %>
  <%= form.number_field :age %>

  <%= form.label :height %>
  <%= form.text_field :height %>

  <%= form.label :injured %>
  <%= form.text_field :injured %>

  <%= form.submit "Player" %>
<% end %>