
<% @players.each do |player|%>

  <%= link_to player.name, "/players/#{player.id}" %>
  <p>Position: <%= player.position %></p>
  <p>Age: <% player.age %></p>
  <p>Height in inches: <%= player.height %></p>
  <p>Injury Status: <%= player.injured %></p>

<% end %>

